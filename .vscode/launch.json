{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (github copilot)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Compass/bin/Debug/net9.0/Compass.dll",
            "args": [
                "--repo-path",".",
                "--config", "./Compass/config/sample-config.json",
                "--runs", "1",
                "--agent-type", "githubcopilot",
                "--model", "gpt-5.1-codex-mini",
                "--eval-model", "gpt-5-mini",
                "--output", "detailed",
                "--verbose"
            ],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
